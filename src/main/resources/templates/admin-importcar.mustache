{{>layout/headeradmin}}

<div class="main">
    <section class="module-small">
        <div class="container">
            <div class="row">
                <div class="col-sm-4 col-md-3 sidebar">
                    <div class="widget">
                        <h5 class="widget-title font-alt">관리자 화면</h5>
                        <ul class="icon-list">
                            <li><a href="/admin">관리자 페이지</a></li>
                            <li><a href="/">ISAM 홈페이지</a></li>
                        </ul>
                    </div>
                    <div class="widget">
                        <h5 class="widget-title font-alt">상품 관리</h5>
                        <ul class="icon-list">
                            <li><a href="/importcar/save">직수입 등록</a></li>
                            <li><a href="/admin/importcar">직수입 리스트</a></li>
                            <li><a href="/usedcar/save">중고차 등록</a></li>
                            <li><a href="/admin/usedcar">중고차 리스트</a></li>
                        </ul>
                    </div>
                    <div class="widget">
                        <h5 class="widget-title font-alt">게시판 관리</h5>
                        <ul class="icon-list">
                            <li><a href="/admin/notice">공지사항 목록</a></li>
                            <li><a href="/admin/news">뉴스 목록</a></li>
                        </ul>
                    </div>
                    <div class="widget">
                        <h5 class="widget-title font-alt">문의사항 관리</h5>
                        <ul class="icon-list">
                            <li><a href="/admin/contactus">일반 문의사항</a></li>
                            <li><a href="/admin/carcontact">차량 문의사항</a></li>
                        </ul>
                    </div>
                    <div class="widget">
                        <h5 class="widget-title font-alt">홈페이지 관리</h5>
                        <ul class="icon-list">
                            <li><a href="#">개발 예정</a></li>
                        </ul>
                    </div>
                </div>
                <div style="float:left;">
                    <div class="carpost">
                        <div class="post-header font-alt">
                            <h1 class="post-title">차량 목록</h1>
                        </div>
                    </div>
                    <div class="post-entr">
                        <table class="table table-striped table-border checkout-table">
                            <tbody>
                            <tr>
                                <th class="hidden-xs">차량사진</th>
                                <th>차량정보</th>
                                <th class="hidden-xs">연식</th>
                                <th>주행거리</th>
                                <th>색상</th>
                                <th>등록일</th>
                                <th>가격</th>
                                <th></th>
                            </tr>
                            {{#galleryList}}
                                <tr>
                                    <td class="hidden-xs"><a href="/importcar/detail/{{id}}"><img src="{{imgFullPath}}" alt="차량이미지"/></a></td>
                                    <td>
                                        <a href="/importcar/detail/{{id}}">
                                            <h7>{{title}}</h7>
                                            <h7>{{{subcontent}}}</h7>
                                            <h7>{{carsalecheck}}</h7>
                                        </a>
                                    </td>
                                    <td class="hidden-xs">
                                        <h7 class="product-title font-alt">{{caryear}}</h7>
                                    </td>
                                    <td>
                                        <h7 class="product-title font-alt">{{cardistance}}</h7>
                                    </td>
                                    <td>
                                        <h7 class="product-title font-alt">{{carcolor}}</h7>
                                    </td>
                                    <td>
                                        <h7 class="product-title font-alt">{{createdDate}}</h7>
                                    </td>
                                    <td>
                                        <h7 class="product-title font-alt">{{carprice}}</h7>
                                    </td>
                                    <td>
                                        <a href="/importcar/update/{{id}}">
                                            <h7 class="product-title font-alt">수정</h7>
                                        </a>
                                        <a href="/importcar/delete/{{id}}">
                                            <h7 class="product-title font-alt">삭제</h7>
                                        </a>
                                    </td>
                                </tr>
                            {{/galleryList}}
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </section>

{{>layout/footeradmin}}