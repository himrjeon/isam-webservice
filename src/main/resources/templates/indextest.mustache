{{>layout/headermain}}


<section class="home-section bg-dark-60 portfolio-page-header parallax-bg" id="home">
    <div class="hero-slider">
        <ul class="slides">
            <li class="bg-dark-30 bg-dark shop-page-header" style="background-image:url(&quot;/image/bently_test.jpg&quot;);">
                <div class="titan-caption">
                    <div class="caption-content">
                        <div class="font-alt mb-30 titan-title-size-1">차량 직수입 전문</div>
                        <div class="font-alt mb-30 titan-title-size-4">아이에스오토모빌은 다릅니다.</div>
                        <!--           <div class="font-alt mb-40 titan-title-size-1">Your online fashion destination</div> -->
                        <a class="section-scroll btn btn-border-w btn-round" href="#latest">더 보기</a>
                    </div>
                </div>
            </li>
            <li class="bg-dark-30 bg-dark shop-page-header" style="background-image:url(&quot;/image/lambo_test.jpg&quot;);">
                <div class="titan-caption">
                    <div class="caption-content">
                        <div class="font-alt mb-30 titan-title-size-1"> IS AutoMobile</div>
                        <div class="font-alt mb-40 titan-title-size-4">extraordinary experience</div><a class="section-scroll btn btn-border-w btn-round" href="#latest">Learn More</a>
                    </div>
                </div>
            </li>
        </ul>
    </div>
</section>

<div class="main">
    <section class="module-small" id="latest">
        <div class="container">
            <div class="row">
                <div class="col-sm-6 col-sm-offset-3">
                    <h2 class="module-title font-alt">직수입차량목록</h2>
                </div>
            </div>
            <div class="row multi-columns-row">
                {{#galleryList}}
                <div class="col-sm-4 col-md-4 col-lg-4">
                    <div class="shop-item">
                        <div class="shop-item-image"><img src="{{imgFullPath}}" alt="Car Photo1"/>
                            <div class="shop-item-detail"><a class="btn btn-round btn-b" href="/importcar/detail/{{id}}">자세히보기</a></div>
                        </div>
                        <h4 class="shop-item-title font-alt"><a href="/importcar/detail/{{id}}">{{title}}</a></h4>
                    </div>
                </div>
                {{/galleryList}}

            </div>
            <div class="row mt-30">
                <div class="col-sm-12 align-center"><a class="btn btn-b btn-round" href="/importcar">직수입 차량 보기</a></div>
            </div>
        </div>
    </section>
    <section class="module-small" id="latest">
        <div class="container">
            <div class="row">
                <div class="col-sm-6 col-sm-offset-3">
                    <h2 class="module-title font-alt">중고차량목록</h2>
                </div>
            </div>
            <div class="row multi-columns-row">
                {{#usedCarList}}
                    <div class="col-sm-4 col-md-4 col-lg-4">
                        <div class="shop-item">
                            <div class="shop-item-image"><img src="{{imgFullPath}}" alt="Car Photo1"/>
                                <div class="shop-item-detail"><a class="btn btn-round btn-b" href="/usedcar/detail/{{id}}">자세히보기</a></div>
                            </div>
                            <h4 class="shop-item-title font-alt"><a href="/usedcar/detail/{{id}}">{{title}}</a></h4>
                        </div>
                    </div>
                {{/usedCarList}}

            </div>
            <div class="row mt-30">
                <div class="col-sm-12 align-center"><a class="btn btn-b btn-round" href="/importcar">중고 차량 보기</a></div>
            </div>
        </div>
    </section>
    <section class="module module-video bg-dark-30" data-background="">
        <div class="container">
            <div class="row">
                <div class="col-sm-6 col-sm-offset-3">
                    <h2 class="module-title font-alt mb-0">수입 진행 및 출고차량</h2>
                    <h2></h2><h2></h2><h2></h2>
                </div>
            </div>
            <div class="col-sm-12 align-center">
            <a class="btn btn-d btn-circle btn-lg" href="#">더 보기</a>
            </div>
        </div>
        <div class="video-player" data-property="{videoURL:'https://youtu.be/cLhKy_f3IVA', containment:'.module-video', startAt:0, mute:true, autoPlay:true, loop:true, opacity:1, showControls:false, showYTLogo:false, vol:25}"></div>
    </section>

    <hr class="divider-w">
    <section class="module" id="news">
        <div class="container">
            <div class="row">
                <div class="col-sm-6 col-sm-offset-3">
                    <h2 class="module-title font-alt">공지사항</h2>
                </div>
            </div>
            <div class="row multi-columns-row post-columns wo-border">
                {{#notice}}
                <div class="col-sm-6 col-md-4 col-lg-4">
                    <div class="post mb-40">
                        <div class="post-header font-alt">
                            <h2 class="post-title"><a href="#">{{title}}</a></h2>
                        </div>
                        <div class="post-entry">
                            {{#content}}
                            <p>{{content}}</p>
                            {{/content}}
                        </div>
                        <div class="post-more"><a class="more-link" href="#">더 보기</a></div>
                    </div>
                </div>
                {{/notice}}
            </div>
        </div>
    </section>
    <hr class="divider-w">
    <section class="module-small">
        <div class="container">
            <div class="row client">
                <div class="owl-carousel text-center" data-items="6" data-pagination="false" data-navigation="false">
                    <div class="owl-item">
                        <div class="col-sm-12">
                            <div class="client-logo"><img src="/image/logo-1.png" alt="Client Logo"/></div>
                        </div>
                    </div>
                    <div class="owl-item">
                        <div class="col-sm-12">
                            <div class="client-logo"><img src="/image/logo-2.png" alt="Client Logo"/></div>
                        </div>
                    </div>
                    <div class="owl-item">
                        <div class="col-sm-12">
                            <div class="client-logo"><img src="/image/logo-3.png" alt="Client Logo"/></div>
                        </div>
                    </div>
                    <div class="owl-item">
                        <div class="col-sm-12">
                            <div class="client-logo"><img src="/image/logo-4.png" alt="Client Logo"/></div>
                        </div>
                    </div>
                    <div class="owl-item">
                        <div class="col-sm-12">
                            <div class="client-logo"><img src="/image/logo-5.png" alt="Client Logo"/></div>
                        </div>
                    </div>
                    <div class="owl-item">
                        <div class="col-sm-12">
                            <div class="client-logo"><img src="/image/logo-10.png" alt="Client Logo"/></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    {{>layout/footermain}}