{{>layout/headermain}}


<div class="main">
    <section class="module">
        <div class="container">
            <div class="row">
                <div class="col-sm-6 col-sm-offset-3">
                    <h1 class="module-title font-alt">중고 차량 목록</h1>
                </div>
            </div>
            <hr class="divider-w pt-20">
            <div class="row">
                <div class="col-sm-12">
                    <table class="table table-striped table-border checkout-table">
                        <tbody>
                        <tr>
                            <th class="hidden-xs">차량사진</th>
                            <th>차량정보</th>
                            <th class="hidden-xs">연식</th>
                            <th>주행거리</th>
                            <th>색상</th>
                            <th>등록일</th>
                        </tr>
                        {{#galleryList}}
                            <tr>
                                <td rowspan="3" class="hidden-xs"><a href="/usedcar/detail/{{id}}"><img src="{{imgFullPath}}" alt="차량이미지"/></a></td>
                                <td rowspan="3">
                                    <a href="/usedcar/detail/{{id}}">
                                        <h5 class="product-title font-alt">{{title}}</h5>
                                        <h7>{{{subcontent}}}</h7>
                                        <h5 class="product-title font-alt">{{carsalecheck}}</h5>
                                    </a>
                                </td>
                                <td class="hidden-xs">
                                    <h5 class="product-title font-alt">{{caryear}}</h5>
                                </td>
                                <td>
                                    <h5 class="product-title font-alt">{{cardistance}}</h5>
                                </td>
                                <td>
                                    <h5 class="product-title font-alt">{{carcolor}}</h5>
                                </td>
                                <td>
                                    <h5 class="product-title font-alt">{{createdDate}}</h5>
                                </td>
                            </tr>
                            <tr>
                                <td colspan="4">
                                    <b>가격</b>
                                </td>
                            </tr>
                            <tr>
                                <td colspan="4">
                                    <h5 class="product-title font-alt">{{carprice}}</h5>
                                </td>
                            </tr>
                        {{/galleryList}}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </section>


{{>layout/footermain}}